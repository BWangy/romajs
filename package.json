    "test:nolint": "mocha --recursive --require babel-core/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "start": "node server.js",
    "build": "rimraf ./build && cross-env NODE_ENV=\"production\" webpack",
